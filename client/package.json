{
  "name": "react-webpack-rails-tutorial",
  "version": "1.1.0",
  "description": "Built using the react_on_rails generator.",
  "main": "server.js",
  "engines": {
    "node": "5.0.0",
    "npm": "3.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shakacode/react-webpack-rails-tutorial.git"
  },
  "keywords": [
    "react",
    "tutorial",
    "comment",
    "example"
  ],
